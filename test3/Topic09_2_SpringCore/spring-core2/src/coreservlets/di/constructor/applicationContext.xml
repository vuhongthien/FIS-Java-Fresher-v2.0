<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean id="impliedValues" class="coreservlets.di.constructor.Values">
    <constructor-arg value="abc" />
    <constructor-arg value="123" />
  </bean>
  
  <bean id="explicitValues" class="coreservlets.di.constructor.Values">
    <constructor-arg type="java.lang.Integer" value="456" />
    <constructor-arg type="java.lang.String" value="123" />
  </bean>
</beans>
